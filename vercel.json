{
  "version": 2,
  "builds": [
    {
      "src": "merchant/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "ui/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "merchant/server.ts"
    },
    {
      "src": "/\\.well-known/(.*)",
      "dest": "merchant/server.ts"
    },
    {
      "src": "/demo",
      "dest": "/"
    },
    {
      "src": "/receipt",
      "dest": "ui/receipt.html"
    },
    {
      "src": "/",
      "dest": "ui/chat.html"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
