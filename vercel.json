{
  "version": 2,
  "builds": [
    {
      "src": "api/ping.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/listings.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/capabilities.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/schemas-invoice-v1.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/invoices.ts",
      "use": "@vercel/node"
    },
    {
      "src": "ui/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/ping",
      "dest": "api/ping.ts"
    },
    {
      "src": "/api/health",
      "dest": "api/health.ts"
    },
    {
      "src": "/api/listings",
      "dest": "api/listings.ts"
    },
    {
      "src": "/api/capabilities",
      "dest": "api/capabilities.ts"
    },
    {
      "src": "/\\.well-known/tempo-agent-payments\\.json",
      "dest": "api/capabilities.ts"
    },
    {
      "src": "/api/schemas/invoice-v1",
      "dest": "api/schemas-invoice-v1.ts"
    },
    {
      "src": "/api/invoices$",
      "dest": "api/invoices.ts"
    },
    {
      "src": "/api/invoices/([^/]+)$",
      "dest": "api/invoices.ts?invoiceId=$1"
    },
    {
      "src": "/api/demo/confirm$",
      "dest": "api/invoices.ts?demoConfirm=1"
    },
    {
      "src": "/api/confirm$",
      "dest": "api/invoices.ts?confirm=1"
    },
    {
      "src": "/demo",
      "dest": "/"
    },
    {
      "src": "/receipt",
      "dest": "ui/receipt.html"
    },
    {
      "src": "/",
      "dest": "ui/chat.html"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
